    // =========================================================================
    // QUESTION SETS
    // =========================================================================

<% for qs in question_sets %>
    // == <%= qs.accronym %>
    // <%= qs.text %>
    QuestionSet <%= qs.accronym.downcase %> = new QuestionSet(QuestionCode.<%= qs.accronym.upcase %>, <%= (qs.repeatable ? 'true' : 'false') %>, <%= qs.parent.accronym.downcase rescue 'null' %>);
    session.saveOrUpdate(<%= qs.accronym.downcase %>);
    <% if qs.parent == nil %>
        form<%= qs.form.number %>.getQuestionSets().add(<%= qs.accronym.downcase %>);
        session.saveOrUpdate(form<%= qs.form.number %>);
    <% end %>

<% end %>