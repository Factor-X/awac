<% if unit_default == nil %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, moneyUnits, <%= default %>, moneyUnits.getMainUnit() ));
<% else %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, moneyUnits, <%= default %>, getUnitBySymbol("<%= unit_default %>") ));
<% end %>