<% if unit_default == nil %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, energyUnits, <%= default %>, energyUnits.getMainUnit() ));
<% else %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, energyUnits, <%= default %>, getUnitBySymbol("<%= unit_default %>") ));
<% end %>