<% if unit_default == nil %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, timeUnits, <%= default %>, timeUnits.getMainUnit() ));
<% else %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, timeUnits, <%= default %>, getUnitBySymbol("<%= unit_default %>") ));
<% end %>