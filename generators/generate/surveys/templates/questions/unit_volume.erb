<% if unit_default == nil %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, volumeUnits, <%= default %>, volumeUnits.getMainUnit() ));
<% else %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, volumeUnits, <%= default %>, getUnitBySymbol("<%= unit_default %>") ));
<% end %>