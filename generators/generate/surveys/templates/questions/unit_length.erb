<% if unit_default == nil %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, lengthUnits, <%= default %>, lengthUnits.getMainUnit() ));
<% else %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, lengthUnits, <%= default %>, getUnitBySymbol("<%= unit_default %>") ));
<% end %>