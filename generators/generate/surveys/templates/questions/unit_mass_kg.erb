<% if unit_default == nil %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, massUnits, <%= default %>, massUnits.getMainUnit() ));
<% else %>
    session.saveOrUpdate(new DoubleQuestion( <%= parent %>, 0, QuestionCode.<%= accronym %>, massUnits, <%= default %>, getUnitBySymbol("<%= unit_default %>") ));
<% end %>