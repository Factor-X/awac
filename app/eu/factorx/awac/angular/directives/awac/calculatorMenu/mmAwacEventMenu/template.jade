.nav_tabs(ng-show='displayMenu===true')
    .nav_entreprise
        .site_menu
            .menu.menu_enterprise
                button.button(ng-class='{\'menu_current\': isMenuCurrentlySelected(\'/form/TAB_EV1\') == true}', ng-click='navTo(\'/form/TAB_EV1\')')
