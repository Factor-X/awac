                    .menu_arrow
                button.button(ng-class='{\'menu_current\': isMenuCurrentlySelected(\'/form/TAB_EV4\') == true}', ng-click='navTo(\'/form/TAB_EV4\')')
                    .tab-title(ng-bind-html='\'TAB_EV4\' | translate')
                    mm-awac-tab-progress-bar(ng-value='getProgress(\'TAB_EV4\')')
                    .menu_arrow
                button.button(ng-class='{\'menu_current\': isMenuCurrentlySelected(\'/form/TAB_EV5\') == true}', ng-click='navTo(\'/form/TAB_EV5\')')
