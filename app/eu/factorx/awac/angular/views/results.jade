ng-virtual
    h1 Results

    table(border="1")
        thead
            th
            th Scope 1
            th Scope 2
            th Scope 3
        tbody
            tr(ng-repeat="rl in o.reportLines")
                td {{rl.indicatorName}}
                td {{rl.scope1Value}}
                td {{rl.scope2Value}}
                td {{rl.scope3Value}}

    fs-donut(bind="temp.browsers")